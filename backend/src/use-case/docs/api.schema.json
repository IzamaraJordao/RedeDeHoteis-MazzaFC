    "paths": {
      "/hotels": {
        "get": {
          "summary": "Get all hotels",
          "description": "Get all hotels",
          "responses": {
            "200": {
              "description": "OK",
              "content": {
                "application/json": {
                  "schema": {
                    "type": "array",
                    "items": {
                      "$ref": "#/use-case/hotel"
                    }
                  }
                }
              }
            }
          }
        },
        "post": {
          "summary": "Create a hotel",
          "description": "Create a hotel",
          "requestBody": {
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/use-case/hotel"
                }
              }
            }
          },
          "responses": {
            "201": {
              "description": "Created",
              "content": {
                "application/json": {
                  "schema": {
                    "$ref": "#/use-case/hotel"
                  }
                }
              }
            }
          }
        }
      },
      "/hotels/{id}": {
